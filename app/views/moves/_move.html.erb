<div id="<%= dom_id move %>">
  <p>
    <strong>Moveid:</strong>
    <%= move.id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= move.name %>
  </p>

  <p>
    <strong>Accuracy:</strong>
    <%= move.accuracy %>
  </p>

  <p>
    <strong>Effect:</strong>
    <%= move.effect %>
  </p>

  <p>
    <strong>Power:</strong>
    <%= move.power %>
  </p>

  <p>
    <strong>Power Points:</strong>
    <%= move.pp %>
  </p>

</div>

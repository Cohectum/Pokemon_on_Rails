<p style="color: green"><%= notice %></p>

<h1>Pokemon</h1>

<div id="pokemons" class="container">
  <% @pokemons.each do |pokemon| %>
    <div class="row tab-row">
      <%= render pokemon %>
      <div class="col-sm">
        <%= link_to "View Pokemon", pokemon %>
      </div>
    </div>
  <% end %>
</div>

<%= link_to "New pokemon", new_pokemon_path %>
